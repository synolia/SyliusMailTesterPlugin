{
    "name": "synolia/sylius-mail-tester-plugin",
    "type": "sylius-plugin",
    "keywords": ["sylius", "sylius-plugin", "mail", "tester"],
    "description": "Mail Tester Plugin.",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "fakerphp/faker": "^1.9",
        "sylius/sylius": "^1.9",
        "symfony/framework-bundle": "^4.4|^5.4",
        "symfony/service-contracts": "^1.1|^2.0"
    },
    "require-dev": {
        "behat/behat": "^3.6.1",
        "behat/mink-selenium2-driver": "^1.4",
        "dmore/behat-chrome-extension": "^1.3",
        "dmore/chrome-mink-driver": "^2.7",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-debug-extension": "^2.0",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/variadic-extension": "^1.3",
        "friendsoftwig/twigcs": "v6.0.0",
        "j13k/yaml-lint": "^1.1",
        "php-parallel-lint/php-parallel-lint": "^1.0",
        "phpmd/phpmd": "2.12.0",
        "phpro/grumphp": "1.13.0",
        "phpspec/phpspec": "7.2.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "1.8.0",
        "phpstan/phpstan-doctrine": "1.3.9",
        "phpstan/phpstan-phpunit": "1.1.1",
        "phpstan/phpstan-strict-rules": "1.3.0",
        "phpstan/phpstan-webmozart-assert": "1.2.0",
        "phpunit/phpunit": "^8.5 || ^9.5",
        "povils/phpmnd": "^2.1",
        "sebastian/phpcpd": "^5.0 || ^6.0",
        "seld/jsonlint": "^1.7",
        "sensiolabs/security-checker": "v6.0.3",
        "sylius-labs/coding-standard": "v4.1.1",
        "symfony/browser-kit": "^4.4|^5.4",
        "symfony/debug-bundle": "^4.4|^5.4|^6.0",
        "symfony/dotenv": "^4.4|^5.4",
        "symfony/lock": "^4.4|^5.4",
        "symfony/web-profiler-bundle": "^4.4|^5.4",
        "symfony/web-server-bundle": "^4.4|^5.4",
        "webmozart/assert": "^1.8"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Synolia\\SyliusMailTesterPlugin\\": "src/",
            "Tests\\Synolia\\SyliusMailTesterPlugin\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-ecs": "ecs check -c ruleset/ecs.php --fix --ansi --clear-cache  ."
    }
}
